.user {
  width: 100%;
  padding-bottom: 200rpx;
}
.user .top {
  padding-top: 20rpx;
  height: 360rpx;
  background-color: #ff7214;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 100%;
}
.user .top .info {
  display: flex;
  align-items: center;
}
.user .top .info .avatar {
  margin-left: 28rpx;
}
.user .top .info .avatar image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
}
.user .top .info .text {
  margin-left: 28rpx;
}
.user .top .info .text .name {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #ffffff;
  line-height: 38rpx;
}
.user .top .info .text .level {
  margin-top: 4rpx;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 38rpx;
}
.user .top .info .more {
  display: flex;
  justify-content: flex-end;
  margin-left: auto;
  margin-right: 28rpx;
}
.user .top .info .more image {
  width: 14rpx;
  height: 25rpx;
}
.user .top .num {
  display: flex;
  align-items: center;
  justify-content: center;
}
.user .top .num .star {
  width: 35%;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 38rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.user .top .num .star .starNum {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #ffffff;
  line-height: 38rpx;
}
.user .center {
  margin-top: -120rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.user .center .background {
  width: 694rpx;
  height: 207rpx;
  background: #ffffff;
  box-shadow: 0px 0px 35rpx 0px rgba(66, 58, 60, 0.08);
  border-radius: 10rpx;
}
.user .center .background .order .title {
  display: flex;
  margin-top: 20rpx;
}
.user .center .background .order .title text {
  margin-left: 28rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 38rpx;
}
.user .center .background .order .title .right {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-left: auto;
  margin-right: 23rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 38rpx;
}
.user .center .background .order .title .right image {
  margin-left: 12rpx;
  width: 9rpx;
  height: 16rpx;
}
.user .center .background .order .orderList {
  display: flex;
  margin-top: 30rpx;
}
.user .center .background .order .orderList .icon {
  width: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.user .center .background .order .orderList .icon image {
  width: 53rpx;
