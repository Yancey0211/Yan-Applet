}
.user .center .background .order .orderList .icon text {
  margin-top: 18rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #666666;
  line-height: 38rpx;
}
.user .center .background .order .orderList .icon .dot {
  font-size: 22rpx;
  position: absolute;
  margin-left: 30rpx;
  margin-top: -50rpx;
  border-radius: 50%;
  min-width: 36rpx;
  height: 36rpx;
  color: #000;
  text-align: center;
  color: #fff;
  background-color: #fb4a4a;
  border: 1px solid #fff;
}
